<section class="read-more">
    <p>
        Read More Posts:
    </p>

    <ul>
        {%- for post in collections.post | reverse -%}
        {% if not (title == post.data.title) %}
        <li>
            <a href="{{ post.url }}">{{ post.data.title }}</a>
        </li>
        {% endif %}
        {%- endfor -%}
    </ul>
</section>